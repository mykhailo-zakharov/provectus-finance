{
  "name": "react-router-ru-tutorial",
  "version": "1.0.0",
  "description": "React-router RU tutorial",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=development&&webpack --config webpack.config.js --progress",
    "build": "set NODE_ENV=production&&webpack --config webpack.production.config.js -p --progress --colors",
    "debug": "set NODE_ENV=DEBUG&&webpack --config webpack.config.js --progress",
    "ubuntu_start": "NODE_ENV=development&&webpack --config webpack.config.js --progress",
    "ubuntu_build": "NODE_ENV=production&&webpack --config webpack.production.config.js -p --progress --colors",
    "lint": "eslint --quiet --ext .js src",
    "lint:fix": "eslint --quiet --fix --ext .js src"
  },
  "author": "Maxim Patsianskiy",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.23.1",
    "eslint": "3.13.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-defaults": "9.0.0",
    "eslint-config-google": "0.7.1",
    "eslint-config-standard": "6.2.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-plugin-react-native": "^3.2.1",
    "eslint-plugin-standard": "2.0.1",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "material-ui": "^0.17.1",
    "node-sass": "^3.4.2",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^0.8.3"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "browser-sync": "^2.12.8",
    "browser-sync-webpack-plugin": "^1.1.3",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.4",
    "react-router": "^2.0.1",
    "redux": "^3.4.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react",
      "react-native"
    ],
    "extends": "airbnb",
    "rules": {
      "no-undef": 0,
      "import/extensions": 0,
      "import/prefer-default-export": 0,
      "import/no-extraneous-dependencies": 0,
      "no-restricted-syntax": [
        "error",
        "ForInStatement",
        "LabeledStatement",
        "WithStatement"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-bind": 0,
      "linebreak-style": 0,
      "react/no-string-refs": 1,     
      "no-prototype-builtins": 0,
      "react/forbid-prop-types": 0      
    }
  }
}
